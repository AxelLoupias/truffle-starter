{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier":[
      "error",
      {
        "endOfLine":"auto"
      }
    ],
    "avoid-suicide": "error",
    "avoid-sha3": "error",
    "func-visibility": ["warn", { "ignoreConstructors": true }],
    "compiler-version":["error","^0.8.11"]
  }
}
